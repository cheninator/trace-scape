{
  "name": "trace-scape",
  "version": "1.0.0",
  "description": "A trace viewer built with web technologies",
  "main": "main.js",
  "scripts": {
    "build": "npm run clean && tsc && gulp build && webpack --devtool source-map",
    "bundle": "echo",
    "clean": "rimraf bin",
    "clean:modules": "rimraf node_modules",
    "desktop": "electron bin/electron/main.js --enable-logging",
    "start": "npm run build && concurrently --kill-others \"tsc -w\" \"npm run web\" ",
    "start:desktop": "npm run build && concurrently --kill-others \"tsc -w\" \"npm run desktop\" ",
    "test": "mocha -r ts-node/register test/test-disks-io.ts",
    "web": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheninator/trace-scape"
  },
  "author": "Yonni Chen",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chart.js":"^2.7.1",
    "concurrently": "3.5.0",
    "golden-layout": "^1.5.9",
    "jquery": "^3.2.1",
    "pixi.js": "^4.6.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.16",
    "@types/mocha": "^2.2.44",
    "@types/pixi.js": "^4.5.7",
    "electron": "^1.7.8",
    "gulp": "^3.9.1",
    "gulp-flatten": "^0.3.1",
    "gulp-uglify": "^3.0.0",
    "lite-server": "^2.3.0",
    "mocha": "^4.0.1",
    "rimraf": "^2.5.4",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "ts-node": "^4.0.1",
    "typescript": "^2.5.3",
    "webpack": "^3.8.1"
  }
}